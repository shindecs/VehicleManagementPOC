@section scripts {
	<script>
		$(document).ready(function () {
			var dataSource = new kendo.data.DataSource({
				data: [
					{ ID: 1, Name: "Option 1" },
					{ ID: 2, Name: "Option 2" },
					{ ID: 3, Name: "Option 3" },
					// More cities...
				],
				sort: { field: "Name", dir: "asc" }
			});

			$("#series").kendoDropDownList({
				filter: "contains",
				optionLabel: 'Please select...',
				dataTextField: "Name",
				dataValueField: "ID",
				dataSource: dataSource
			});
			$("#description").kendoDropDownList({
				filter: "contains",
				optionLabel: 'Please select...',
				dataTextField: "Name",
				dataValueField: "ID",
				dataSource: dataSource
			});
			$("#engine").kendoDropDownList({
				filter: "contains",
				optionLabel: 'Please select...',
				dataTextField: "Name",
				dataValueField: "ID",
				dataSource: dataSource
			});
			$("#vd").kendoDropDownList({
				filter: "contains",
				optionLabel: 'Please select...',
				dataTextField: "Name",
				dataValueField: "ID",
				dataSource: dataSource
			});
			$("#transmission").kendoDropDownList({
				filter: "contains",
				optionLabel: 'Please select...',
				dataTextField: "Name",
				dataValueField: "ID",
				dataSource: dataSource
			});
			$("#vmodel").kendoDropDownList({
